{
	"presets": [
		[
			"@babel/preset-env",
			{
				"useBuiltIns": "usage",
				"corejs": "3.0.0"
			}
		],
		"@babel/preset-react"
	],
	"plugins": [
		[
			"@babel/plugin-transform-runtime", // 修复babel的两个bug（常用）
			{
				"corejs": 3 // 指定 runtime-corejs 的版本，目前有 2 3 两个版本
			}
		],
		[
			"@babel/plugin-proposal-decorators", // 装饰器
			{
				"legacy": true
			}
		]
	]
}